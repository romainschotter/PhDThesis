@article{alicecollaborationMultiplicityDependence8922019,
  title = {Multiplicity Dependence of {{K}}*(892)\$\^\{0\}\$ and \$\textbackslash phi\$(1020) Production in Pp Collisions at \$\textbackslash sqrt\{s\}\$ = 13 {{TeV}}},
  author = {ALICE Collaboration},
