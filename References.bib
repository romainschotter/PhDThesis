  file = {/home/rschotte/snap/zotero-snap/common/Zotero/storage/BVGHY9UU/al and Collaboration) - 2014 - Upgrade of the ALICE Experiment Letter Of Intent.pdf}
}

@article{alicecollaborationValidationALICEMaterial2022,
